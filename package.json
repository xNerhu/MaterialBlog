{
  "name": "MaterialBlog",
  "version": "2.0.0",
  "description": "Project created for my school class (Albert Einstein WSERO Laungage Secondary School) on Google's Material Design style",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index",
    "watch": "npm run build:dev && webpack-dev-server --progress --colors --hot --inline --port 8080 --history-api-fallback",
    "build:dev": "rimraf build && webpack",
    "build:production": "rimraf build && cross-env NODE_ENV=production webpack --progress --define process.env.NODE_ENV=production",
    "save": "node save",
    "default-settings": "node default-settings"
  },
  "keywords": [
    "rest-api",
    "cms",
    "react",
    "webpack",
    "sass",
    "blog",
    "einstein",
    "wsero",
    "opole",
    "gimnazjum",
    "mikolaj",
    "palkiewicz",
    "xnerhu"
  ],
  "author": "Nersent",
  "license": "MIT",
  "dependencies": {
    "archiver": "latest",
    "babel-polyfill": "latest",
    "bcrypt": "latest",
    "body-parser": "latest",
    "cookie-parser": "latest",
    "ejs": "latest",
    "express": "latest",
    "express-session": "latest",
    "helmet": "latest",
    "history": "latest",
    "mongodb": "latest",
    "passport": "latest",
